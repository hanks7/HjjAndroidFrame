    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_web_base"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.taoxue.WebBaseActivity">

    <com.taoxue.umeng.view.TopBar
        android:id="@+id/top_bar"
        style="@style/TopBar"
        app:title="web" />

    <com.taoxue.umeng.view.ProgressBarWebView
        android:id="@+id/lib_home_web_view"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="match_parent"/>
</LinearLayout>
